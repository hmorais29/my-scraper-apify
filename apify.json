{
  "name": "my-scraper-imoveis",
  "version": "0.0",
  "buildTag": "latest",
  "env": {
    "NODE_ENV": "production"
  },
  "template": "puppeteer",
  "dockerfile": "apify/actor-node-puppeteer-chrome:latest",
  "input": {
    "title": "Consulta de Imóvel",
    "type": "object",
    "schema": {
      "type": "object",
      "properties": {
        "query": {
          "type": "string",
          "title": "Consulta de Pesquisa",
          "description": "A consulta de pesquisa para imóveis (ex: 'Imóvel T4 Cascais novo')."
        }
      },
      "required": ["query"]
    }
  }
}
